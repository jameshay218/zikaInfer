file = {:home/james/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Miner et al. - 2016 - Zika Virus Infection during Pregnancy in Mice Causes Placental Damage and Fetal Demise.pdf:pdf},
issn = {00928674},
journal = {Cell},
number = {5},
pages = {1081--1091},
title = {{Zika Virus Infection during Pregnancy in Mice Causes Placental Damage and Fetal Demise}},
volume = {165},
year = {2016}
}
