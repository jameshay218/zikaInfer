issn = {1553-7374},
journal = {PLoS Pathog},
keywords = {Adolescent,Adult,Aged,Antibodies,Cohort Studies,Female,Hemagglutinin Glycoproteins,Human,Human: blood,Human: epidemiology,Human: immunology,Humans,Influenza,Influenza Virus,Influenza Virus: bloo,Influenza Virus: immu,Male,Measurement error,Middle Aged,Observer Variation,Seroepidemiologic Studies,Serologic Tests,Vietnam,Vietnam: epidemiology,Viral,Viral: blood,Viral: immunology},
