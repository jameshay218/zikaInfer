isbn = {1873-2518 (Electronic)$\backslash$r0264-410X (Linking)},
issn = {0264410X},
journal = {Vaccine},
keywords = {Antibody standard,Haemagglutination-inhibition,Influenza,Neutralisation,Pandemic,Serology},
number = {2},
