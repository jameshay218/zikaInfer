pages = {2851--2858},
title = {{Efficacy of vaccination with different combinations of {\{}MF59-adjuvanted{\}} and nonadjuvanted seasonal and pandemic influenza vaccines against pandemic {\{}H1N1{\}} (2009) influenza virus infection in ferrets}},
volume = {85},
year = {2011}
}
