pmid = {24990904},
title = {{Immunogenicity of the Trivalent Inactivated Influenza Vaccine in Young Children Less than 4 Years of Age, with a Focus on Age and Baseline Antibodies}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24990904 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4178574 http://cvi.asm.org/cgi/doi/10.1128/CVI.00200-14},
