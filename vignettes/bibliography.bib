month = {apr},
number = {15},
pages = {5935--40},
pmid = {16585506},
publisher = {National Academy of Sciences},
title = {{Mitigation strategies for pandemic influenza in the United States.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16585506 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC1458676},
volume = {103},
year = {2006}
}
