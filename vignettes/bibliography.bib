month = {dec},
number = {12},
pages = {1081--1085},
pmid = {21983214},
title = {{Hemagglutination Inhibition Antibody Titers as a Correlate of Protection for Inactivated Influenza Vaccines in Children}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21983214 http://content.wkhealth.com/linkback/openurl?sid=WKPTLP:landingpage{\&}an=00006454-201112000-00013},
volume = {30},
