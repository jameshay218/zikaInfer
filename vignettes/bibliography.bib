issn = {2164-554X},
journal = {Hum. Vaccin. Immunother.},
number = {7},
pages = {1512--1522},
pmid = {23571166},
title = {{Long-term persistence of humoral and cellular immune responses induced by an AS03A-adjuvanted H1N1 2009 influenza vaccine: an open-label, randomized study in adults aged 18-60 years and older.}},
